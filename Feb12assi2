a=21
b=10


if ( a == b ):
   print ("a is equal to b")
else:
   print ("a is not equal to b")

if ( a != b ):
   print ("a is not equal to b")
else:
   print (" a is equal to b") 

   

if ( a > b ):
   print ("a is greater than b") 
else:
   print ("a is not greater than b")

if ( a < b ):
   print ("a is smaller than b" )
else:
   print (" a is not smaller  than b")
